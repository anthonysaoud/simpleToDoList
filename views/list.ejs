<%- include("header") -%>


<body>
    <div class="box" id="heading">
        <h1>
            <%= ListTitle %>
        </h1>
    </div>

    <div class="box">
        
        <% for (i=0;i<newListItem.length;i++) {%>
            <div classs = "item">
                <input type="checkbox">
                <label><%= newListItem[i] %></label>
                
            </div>    
             <% } %>

    <form class="item" action="/" method="post">
        <input class="" type="text" name="newItem" placeholder = "New Item" autocomplete="on" >
        <button type="submit" name = "button" value = <%= ListTitle %> >+</button>
    </form>
                
    </div>


    <%- include("footer") -%>
  


